package ar.edu.unlp.objetos.uno.ejercicio7;

public class Cuadrado extends Figura2d{

	private double lado;
	
	public Cuadrado () {
		
	}
	
	@Override
	public double getPerimetro() {
		return lado * 4;
	}

	@Override
	public double getArea() {
		return lado * lado;
	}
	
	public void setLado(double lado) {
		this.lado = lado;
	}
	
}
